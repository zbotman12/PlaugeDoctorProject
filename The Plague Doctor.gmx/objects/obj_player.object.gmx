            <kind>1</kind>
            <string>///Main State Machine

switch (state)
{
    case states.normal:{ 
        //Behavior
        scr_motion();
        
        if(!place_meeting(x,y,obj_ladder_mask) || grounded){
            justOnLadder = false;
        }
        //Transitions
        if(((position_meeting(x,bbox_top,obj_ladder_mask) &amp;&amp; up ) || (position_meeting(x,bbox_bottom,obj_ladder_mask) &amp;&amp; down)) &amp;&amp; !justOnLadder &amp;&amp; !jump)
        {
            justOnLadder = true;
            ladderSnap = true;
            turnDelayTimer = 0;
            vspd = 0;
            hspd = 0;
            state = states.ladder;
        }
         
    }break;
    case states.staircase:{
    
    }break;
    case states.ladder:{
        //Behavior
        //Snap to ladder's X
        var ladderObj = instance_place(x,y, obj_ladder_mask);
        if(ladderObj != noone &amp;&amp; ladderSnap){
            x = ladderObj.x;
            ladderSnap = false;
        }
        //Going up and down
        if(turnDelayTimer != 0){
            turnDelayTimer--;
        }else if(up &amp;&amp; !down){
            if(lastkey == "down"){
                turnDelayTimer = turnDelayTime;
            }else{
                vspd = -ladderspd;
            }
            lastkey = "up";
        }else if (down &amp;&amp; !up){
            if(lastkey =="up"){
                turnDelayTimer = turnDelayTime;
            }else{
                vspd = ladderspd;
            }
            lastkey = "down";
        }
         
         //Disable upward movement if high enough on ladder
         if(!position_meeting(x, y - ladderspd, obj_ladder_mask) &amp;&amp; position_meeting(x, y, obj_ladder_mask)){
            //If your above the ladder
            if(vspd &lt; 0){
                vspd = 0;
            }
         }
        
         //disable all movement if no input up or down
        if((!up &amp;&amp; !down) || (up &amp;&amp; down)){
            vspd = 0;
        }
        
        //Transitions
        if((jump)){
            jumped = true;
            grounded = false;
           turnDelayTimer = 0;
           state = states.normal;
        }
       //fall off if low enough on ladder
       if(!position_meeting(x ,y, obj_ladder_mask) &amp;&amp; position_meeting(x ,y - sprite_get_height(spr_player_mask) / 2, obj_ladder_mask) &amp;&amp; down){
            turnDelayTimer = 0;
            state = states.normal;
       }
       if(!up &amp;&amp; !down || (up &amp;&amp; down)){
            if(right &amp;&amp; !position_meeting(x + sprite_get_width(obj_ladder_mask)/2 + 1,y, obj_collision_box)){
                turnDelayTimer = 0;
                state = states.normal;
            }else if(left &amp;&amp; !position_meeting(x - sprite_get_width(obj_ladder_mask)/2 - 1,y, obj_collision_box)){
                turnDelayTimer = 0;
                state = states.normal;
            }
       }
       
        if(place_meeting(x, y + vspd, obj_collision_box)){
            while(!place_meeting(x, y+sign(vspd), obj_collision_box)){
                y += sign(vspd);
            }
            vspd = 0;
        }
        
        y += vspd;
        
        
    }break;
    case states.ledgegrab:{
    
    }break;
    case states.hide:{
         
    }break;
}
